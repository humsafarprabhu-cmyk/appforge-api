    {
      "name": "Exercises",
      "html": "<!DOCTYPE html><html lang=\"en\"><head><meta charset=\"UTF-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no\"><title>FitPulse - Exercises</title><style>*{margin:0;padding:0;box-sizing:border-box}body{background:#050507;color:#fff;font-family:-apple-system,BlinkMacSystemFont,'Segoe UI',Roboto,sans-serif;min-height:100vh;padding-bottom:80px}.header{padding:20px}.header h1{font-size:20px;font-weight:600}.search{padding:0 20px 16px}.search input{width:100%;padding:14px 14px 14px 44px;background:rgba(255,255,255,0.04);border:1px solid rgba(255,255,255,0.08);border-radius:14px;color:#fff;font-size:14px;outline:none;font-family:inherit}.search input:focus{border-color:#10b981}.search{position:relative}.search::before{content:'üîç';position:absolute;left:34px;top:14px;font-size:16px}.filter-tabs{display:flex;gap:8px;padding:0 20px 20px;overflow-x:auto}.filter-tab{padding:10px 18px;border-radius:12px;background:rgba(255,255,255,0.04);font-size:13px;cursor:pointer;white-space:nowrap;transition:all 0.3s;border:1px solid rgba(255,255,255,0.06)}.filter-tab.active{background:linear-gradient(135deg,#059669,#10b981);border-color:transparent}.exercise-grid{padding:0 20px}.ex-card{display:flex;align-items:center;gap:14px;padding:16px;margin-bottom:10px;background:rgba(255,255,255,0.04);border:1px solid rgba(255,255,255,0.06);border-radius:16px;cursor:pointer;transition:all 0.3s;animation:fadeIn 0.3s ease backwards}.ex-card:hover{background:rgba(255,255,255,0.06)}@keyframes fadeIn{from{opacity:0;transform:translateY(6px)}to{opacity:1;transform:translateY(0)}}.ex-card .icon{width:52px;height:52px;border-radius:14px;display:flex;align-items:center;justify-content:center;font-size:24px}.ex-card .icon.c{background:rgba(251,146,60,0.15)}.ex-card .icon.b{background:rgba(56,189,248,0.15)}.ex-card .icon.l{background:rgba(139,92,246,0.15)}.ex-card .icon.a{background:rgba(52,211,153,0.15)}.ex-card .icon.s{background:rgba(244,114,182,0.15)}.ex-card .info{flex:1}.ex-card .info .name{font-size:14px;font-weight:500;margin-bottom:2px}.ex-card .info .muscle{font-size:12px;color:rgba(255,255,255,0.4)}.ex-card .info .tags{display:flex;gap:6px;margin-top:6px}.ex-card .info .tag{padding:3px 8px;background:rgba(255,255,255,0.04);border-radius:6px;font-size:10px;color:rgba(255,255,255,0.3)}.ex-card .arrow{color:rgba(255,255,255,0.2)}.nav{position:fixed;bottom:0;left:0;right:0;height:70px;background:rgba(5,5,7,0.9);backdrop-filter:blur(20px);border-top:1px solid rgba(255,255,255,0.06);display:flex;justify-content:space-around;align-items:center}.nav-item{display:flex;flex-direction:column;align-items:center;gap:4px;cursor:pointer;padding:8px 16px;border-radius:12px;transition:all 0.3s}.nav-item.active{background:rgba(16,185,129,0.15)}.nav-item .icn{font-size:20px}.nav-item .lbl{font-size:10px;color:rgba(255,255,255,0.4)}.nav-item.active .lbl{color:#34d399}</style></head><body><div class=\"header\"><h1>Exercise Library</h1></div><div class=\"search\"><input type=\"text\" placeholder=\"Search exercises...\" id=\"searchInput\" oninput=\"filterExercises()\"></div><div class=\"filter-tabs\" id=\"filterTabs\"></div><div class=\"exercise-grid\" id=\"exGrid\"></div><nav class=\"nav\"><div class=\"nav-item\" onclick=\"navigate('Dashboard')\"><span class=\"icn\">üè†</span><span class=\"lbl\">Home</span></div><div class=\"nav-item\" onclick=\"navigate('Log Workout')\"><span class=\"icn\">üèãÔ∏è</span><span class=\"lbl\">Log</span></div><div class=\"nav-item active\" onclick=\"navigate('Exercises')\"><span class=\"icn\">üìã</span><span class=\"lbl\">Exercises</span></div><div class=\"nav-item\" onclick=\"navigate('Progress')\"><span class=\"icn\">üìà</span><span class=\"lbl\">Progress</span></div><div class=\"nav-item\" onclick=\"navigate('Settings')\"><span class=\"icn\">‚öôÔ∏è</span><span class=\"lbl\">Settings</span></div></nav><script>function navigate(s){window.parent.postMessage({type:'navigate',screen:s},'*')}let activeFilter='all';const exercises=[{name:'Bench Press',muscle:'Chest',group:'chest',icon:'c',tags:['Compound','Barbell']},{name:'Incline Dumbbell Press',muscle:'Upper Chest',group:'chest',icon:'c',tags:['Compound','Dumbbell']},{name:'Cable Flyes',muscle:'Chest',group:'chest',icon:'c',tags:['Isolation','Cable']},{name:'Push Ups',muscle:'Chest, Triceps',group:'chest',icon:'c',tags:['Bodyweight']},{name:'Deadlift',muscle:'Back, Legs',group:'back',icon:'b',tags:['Compound','Barbell']},{name:'Pull Ups',muscle:'Lats',group:'back',icon:'b',tags:['Bodyweight','Compound']},{name:'Barbell Row',muscle:'Back',group:'back',icon:'b',tags:['Compound','Barbell']},{name:'Lat Pulldown',muscle:'Lats',group:'back',icon:'b',tags:['Compound','Cable']},{name:'Squats',muscle:'Quads, Glutes',group:'legs',icon:'l',tags:['Compound','Barbell']},{name:'Leg Press',muscle:'Quads',group:'legs',icon:'l',tags:['Compound','Machine']},{name:'Lunges',muscle:'Quads, Glutes',group:'legs',icon:'l',tags:['Compound','Dumbbell']},{name:'Leg Curl',muscle:'Hamstrings',group:'legs',icon:'l',tags:['Isolation','Machine']},{name:'Bicep Curls',muscle:'Biceps',group:'arms',icon:'a',tags:['Isolation','Dumbbell']},{name:'Tricep Pushdown',muscle:'Triceps',group:'arms',icon:'a',tags:['Isolation','Cable']},{name:'Hammer Curls',muscle:'Biceps',group:'arms',icon:'a',tags:['Isolation','Dumbbell']},{name:'Shoulder Press',muscle:'Shoulders',group:'shoulders',icon:'s',tags:['Compound','Dumbbell']},{name:'Lateral Raises',muscle:'Side Delts',group:'shoulders',icon:'s',tags:['Isolation','Dumbbell']},{name:'Face Pulls',muscle:'Rear Delts',group:'shoulders',icon:'s',tags:['Isolation','Cable']}];const groups=['all','chest','back','legs','arms','shoulders'];function renderFilters(){document.getElementById('filterTabs').innerHTML=groups.map(g=>`<div class=\"filter-tab ${g===activeFilter?'active':''}\" onclick=\"setFilter('${g}')\">${g.charAt(0).toUpperCase()+g.slice(1)}</div>`).join('')}function setFilter(g){activeFilter=g;renderFilters();renderExercises()}function filterExercises(){renderExercises()}function renderExercises(){const q=(document.getElementById('searchInput').value||'').toLowerCase();const filtered=exercises.filter(e=>{const matchGroup=activeFilter==='all'||e.group===activeFilter;const matchSearch=!q||e.name.toLowerCase().includes(q)||e.muscle.toLowerCase().includes(q);return matchGroup&&matchSearch});document.getElementById('exGrid').innerHTML=filtered.map(e=>`<div class=\"ex-card\"><div class=\"icon ${e.icon}\">${e.group==='chest'?'üí™':e.group==='back'?'üîô':e.group==='legs'?'ü¶µ':e.group==='arms'?'üí™':'üèîÔ∏è'}</div><div class=\"info\"><div class=\"name\">${e.name}</div><div class=\"muscle\">${e.muscle}</div><div class=\"tags\">${e.tags.map(t=>`<span class=\"tag\">${t}</span>`).join('')}</div></div><div class=\"arrow\">‚Ä∫</div></div>`).join('')}renderFilters();renderExercises();</script></body></html>"
