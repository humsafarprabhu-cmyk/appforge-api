      "name": "Log Workout",
      "html": "<!DOCTYPE html><html lang=\"en\"><head><meta charset=\"UTF-8\"><meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no\"><title>FitPulse - Log</title><style>*{margin:0;padding:0;box-sizing:border-box}body{background:#050507;color:#fff;font-family:-apple-system,BlinkMacSystemFont,'Segoe UI',Roboto,sans-serif;min-height:100vh;padding-bottom:80px}.header{padding:20px;display:flex;align-items:center;gap:16px}.header .back{font-size:24px;cursor:pointer;width:40px;height:40px;display:flex;align-items:center;justify-content:center;border-radius:12px;background:rgba(255,255,255,0.04)}.header h1{font-size:20px;font-weight:600}.workout-type{display:grid;grid-template-columns:repeat(2,1fr);gap:10px;padding:0 20px;margin-bottom:24px}.type-card{padding:18px;background:rgba(255,255,255,0.04);border:1px solid rgba(255,255,255,0.06);border-radius:16px;text-align:center;cursor:pointer;transition:all 0.3s}.type-card.active{background:rgba(16,185,129,0.1);border-color:#10b981}.type-card .emoji{font-size:28px;margin-bottom:6px}.type-card .name{font-size:13px;font-weight:500}.form{padding:0 20px}.form-group{margin-bottom:18px}.form-group label{display:block;font-size:13px;color:rgba(255,255,255,0.5);margin-bottom:8px}.form-group input{width:100%;padding:16px;background:rgba(255,255,255,0.04);border:1px solid rgba(255,255,255,0.08);border-radius:14px;color:#fff;font-size:15px;outline:none;font-family:inherit}.form-group input:focus{border-color:#10b981}.exercise-list{margin-bottom:20px}.ex-title{font-size:14px;font-weight:600;margin-bottom:12px;display:flex;justify-content:space-between;align-items:center}.ex-title .add-ex{color:#34d399;font-size:13px;cursor:pointer}.exercise-item{display:flex;align-items:center;gap:12px;padding:14px;margin-bottom:8px;background:rgba(255,255,255,0.04);border:1px solid rgba(255,255,255,0.06);border-radius:14px}.exercise-item .num{width:28px;height:28px;border-radius:50%;background:rgba(16,185,129,0.15);display:flex;align-items:center;justify-content:center;font-size:12px;color:#34d399;font-weight:600}.exercise-item .ex-info{flex:1}.exercise-item .ex-info .ex-name{font-size:14px;font-weight:500}.exercise-item .ex-info .ex-detail{font-size:12px;color:rgba(255,255,255,0.4);margin-top:2px}.exercise-item .remove{color:rgba(255,255,255,0.2);cursor:pointer;font-size:18px}.save-btn{width:calc(100% - 40px);margin:10px 20px 30px;padding:18px;background:linear-gradient(135deg,#059669,#10b981);border:none;border-radius:16px;color:#fff;font-size:16px;font-weight:600;cursor:pointer;font-family:inherit;transition:all 0.3s}.save-btn:hover{transform:translateY(-2px);box-shadow:0 8px 24px rgba(16,185,129,0.3)}.toast{position:fixed;top:20px;left:50%;transform:translateX(-50%) translateY(-100px);background:linear-gradient(135deg,#059669,#10b981);padding:14px 28px;border-radius:14px;font-size:14px;transition:transform 0.5s cubic-bezier(0.34,1.56,0.64,1);z-index:100}.toast.show{transform:translateX(-50%) translateY(0)}.nav{position:fixed;bottom:0;left:0;right:0;height:70px;background:rgba(5,5,7,0.9);backdrop-filter:blur(20px);border-top:1px solid rgba(255,255,255,0.06);display:flex;justify-content:space-around;align-items:center}.nav-item{display:flex;flex-direction:column;align-items:center;gap:4px;cursor:pointer;padding:8px 16px;border-radius:12px;transition:all 0.3s}.nav-item.active{background:rgba(16,185,129,0.15)}.nav-item .icon{font-size:20px}.nav-item .label{font-size:10px;color:rgba(255,255,255,0.4)}.nav-item.active .label{color:#34d399}</style></head><body><div class=\"header\"><div class=\"back\" onclick=\"navigate('Dashboard')\">‚Üê</div><h1>Log Workout</h1></div><div class=\"workout-type\" id=\"typeGrid\"></div><div class=\"form\"><div class=\"form-group\"><label>Workout Name</label><input type=\"text\" id=\"wName\" placeholder=\"e.g., Push Day\"></div><div class=\"form-group\"><label>Duration (minutes)</label><input type=\"number\" id=\"wDuration\" placeholder=\"45\"></div></div><div style=\"padding:0 20px\"><div class=\"ex-title\">Exercises <span class=\"add-ex\" onclick=\"addExercise()\">+ Add</span></div><div id=\"exerciseList\"></div></div><button class=\"save-btn\" onclick=\"saveWorkout()\">Save Workout üí™</button><div class=\"toast\" id=\"toast\">‚úÖ Workout logged!</div><nav class=\"nav\"><div class=\"nav-item\" onclick=\"navigate('Dashboard')\"><span class=\"icon\">üè†</span><span class=\"label\">Home</span></div><div class=\"nav-item active\" onclick=\"navigate('Log Workout')\"><span class=\"icon\">üèãÔ∏è</span><span class=\"label\">Log</span></div><div class=\"nav-item\" onclick=\"navigate('Exercises')\"><span class=\"icon\">üìã</span><span class=\"label\">Exercises</span></div><div class=\"nav-item\" onclick=\"navigate('Progress')\"><span class=\"icon\">üìà</span><span class=\"label\">Progress</span></div><div class=\"nav-item\" onclick=\"navigate('Settings')\"><span class=\"icon\">‚öôÔ∏è</span><span class=\"label\">Settings</span></div></nav><script>function navigate(s){window.parent.postMessage({type:'navigate',screen:s},'*')}let selectedType='chest';const types=[{id:'chest',emoji:'üí™',name:'Chest'},{id:'back',emoji:'üîô',name:'Back'},{id:'legs',emoji:'ü¶µ',name:'Legs'},{id:'cardio',emoji:'üèÉ',name:'Cardio'},{id:'shoulders',emoji:'üèîÔ∏è',name:'Shoulders'},{id:'arms',emoji:'üí™',name:'Arms'}];let exercises=[{name:'Bench Press',sets:4,reps:12},{name:'Incline Dumbbell',sets:3,reps:10},{name:'Cable Flyes',sets:3,reps:15}];function renderTypes(){document.getElementById('typeGrid').innerHTML=types.map(t=>`<div class=\"type-card ${t.id===selectedType?'active':''}\" onclick=\"selectType('${t.id}')\"><div class=\"emoji\">${t.emoji}</div><div class=\"name\">${t.name}</div></div>`).join('')}function selectType(t){selectedType=t;renderTypes()}function renderExercises(){document.getElementById('exerciseList').innerHTML=exercises.map((e,i)=>`<div class=\"exercise-item\"><div class=\"num\">${i+1}</div><div class=\"ex-info\"><div class=\"ex-name\">${e.name}</div><div class=\"ex-detail\">${e.sets} sets √ó ${e.reps} reps</div></div><div class=\"remove\" onclick=\"removeExercise(${i})\">√ó</div></div>`).join('')}function addExercise(){const name=prompt('Exercise name:');if(!name)return;const sets=parseInt(prompt('Sets:'))||3;const reps=parseInt(prompt('Reps:'))||10;exercises.push({name,sets,reps});renderExercises()}function removeExercise(i){exercises.splice(i,1);renderExercises()}function saveWorkout(){const name=document.getElementById('wName').value.trim()||`${selectedType.charAt(0).toUpperCase()+selectedType.slice(1)} Day`;const duration=parseInt(document.getElementById('wDuration').value)||45;const w=JSON.parse(localStorage.getItem('fp_workouts')||'[]');w.unshift({id:Date.now(),name,type:selectedType,exercises:exercises.length,duration,calories:Math.round(duration*8.5),date:new Date().toISOString().split('T')[0]});localStorage.setItem('fp_workouts',JSON.stringify(w));const toast=document.getElementById('toast');toast.classList.add('show');setTimeout(()=>{toast.classList.remove('show');navigate('Dashboard')},1200)}renderTypes();renderExercises();</script></body></html>"
